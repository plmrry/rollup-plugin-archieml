{
  "name": "rollup-plugin-archieml",
  "type": "module",
  "version": "1.1.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Import ArchieML files using Rollup",
  "keywords": [
    "archieml",
    "rollup"
  ],
  "license": "MIT",
  "repository": "plmrry/rollup-plugin-archieml",
  "author": "Paul Murray",
  "homepage": "https://github.com/plmrry/rollup-plugin-archieml#readme",
  "bugs": "https://github.com/plmrry/rollup-plugin-archieml/issues",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npx tsc && rollup -c",
    "prepare": "rm -rf dist && npm run build",
    "pretest": "npm run prepare",
    "release": "npm publish",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "archieml": "0.5.0"
  },
  "devDependencies": {
    "@rollup/pluginutils": "latest",
    "@types/estree": "latest",
    "jest": "latest",
    "rollup": "latest",
    "typescript": "latest"
  },
  "types": "./dist/index.d.ts",
  "jest": {
    "transform": {},
    "automock": false,
    "clearMocks": true,
    "verbose": true
  }
}
